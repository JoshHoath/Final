{
  "addons": [
    "cleardb",
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/php"
    }
  ],
  "env": {
    "APP_DEBUG": {
      "required": true
    },
    "APP_ENV": {
      "required": true
    },
    "APP_KEY": {
      "required": true
    },
    "APP_NAME": {
      "required": true
    },
    "APP_URL": {
      "required": true
    },
    "BROADCAST_DRIVER": {
      "required": true
    },
    "CACHE_DRIVER": {
      "required": true
    },
    "CLEARDB_DATABASE_URL": {
      "required": true
    },
    "DB_CONNECTION": {
      "required": true
